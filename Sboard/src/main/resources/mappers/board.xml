<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.co.sboard.dao.BoardDao">
	<insert id="insertArticle">
		INSERT INTO `article` SET 
			`title`=#{title},
			`content`=#{content},
			`uid`=#{uid},
			`regip`=#{regip},
			`rdate`=NOW();
	</insert>
	
	<select id="selectArticle" resultType="kr.co.sboard.vo.ArticleVo">
	</select>
	<select id="selectArticles" resultType="kr.co.sboard.vo.ArticleVo">
	</select>
	
	<update id="updateArticle"></update>
	<delete id="deleteArticle"></delete>
</mapper>






